import java.util.*;
class Frame
{
  public static void main (String[]args)
  {
    Scanner sc = new Scanner (System.in);
    int n;
      LinkedList < Integer > x1 = new LinkedList < Integer > ();
      System.out.print ("ENTER N VALUE:");
      n = sc.nextInt ();
    for (int i = 0; i < n; i++)
      {
	x1.add (sc.nextInt ());
      }
    for (Integer i:x1)
      {
	System.out.println (i);
      }
    x1.remove (4);
    System.out.println (x1);
  }
}
---------------------------------------------------------------------------------------------------------------------------------------------------------
OUTPUT:
C:\Users\hp>javac Frame.java
C:\Users\hp>java Frame
ENTER N VALUE:5
10 20 30 40 50
[10, 20, 30, 40]

